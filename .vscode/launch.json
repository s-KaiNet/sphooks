{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run ts-node",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/ts-node/dist/_bin.js",
			"stopOnEntry": false,
			"args": [
				"src/index.ts",
				"view",
				"--list",
				"sites/hr/lists/qa"
			],
			"cwd": "${workspaceRoot}",
			"protocol": "inspector"
		}
	]
}